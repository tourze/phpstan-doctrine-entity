{
  "name": "tourze/phpstan-doctrine-entity",
  "description": "PHPStan rules for Doctrine Entity validation",
  "type": "library",
  "homepage": "https://github.com/tourze/phpstan-doctrine-entity",
  "license": "MIT",
  "require": {
    "composer-runtime-api": "^2.0",
    "doctrine/dbal": "^4.0",
    "doctrine/orm": "^3.0",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "tourze/phpunit-doctrine-entity": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/enum-extra": "0.1.*",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-enum": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\PHPStanDoctrineEntity\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\PHPStanDoctrineEntity\\Tests\\": "tests/"
    }
  },
  "extra": {
    "release-version": "0.1.*"
  }
}